{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "yarn",
    "analytics": false
  },
  "projects": {
    "admin-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "admin-app",
      "sourceRoot": "admin-app/src",
      "prefix": "mys",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": ["moment"],
            "outputPath": "dist/admin-app",
            "index": "admin-app/src/index.html",
            "main": "admin-app/src/main.ts",
            "polyfills": "admin-app/src/polyfills.ts",
            "tsConfig": "admin-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "admin-app/src/favicon.ico",
              "admin-app/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/tinymce",
                "output": "/tinymce/"
              }
            ],
            "styles": [
              "admin-app/src/styles.scss",
              "node_modules/@ngneat/hot-toast/src/styles/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "admin-app/src/environments/environment.ts",
                  "with": "admin-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "admin-app:build:production"
            },
            "development": {
              "browserTarget": "admin-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "admin-app/src/test.ts",
            "polyfills": "admin-app/src/polyfills.ts",
            "tsConfig": "admin-app/tsconfig.spec.json",
            "karmaConfig": "admin-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["admin-app/src/favicon.ico", "admin-app/src/assets"],
            "styles": ["admin-app/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "site-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "site-app",
      "sourceRoot": "site-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": ["flat"],
            "outputPath": "dist/site-app/browser",
            "index": "site-app/src/index.html",
            "main": "site-app/src/main.ts",
            "polyfills": "site-app/src/polyfills.ts",
            "tsConfig": "site-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["site-app/src/favicon.ico", "site-app/src/assets"],
            "styles": ["site-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "site-app/src/environments/environment.ts",
                  "with": "site-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "site-app:build:production"
            },
            "development": {
              "browserTarget": "site-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "site-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "site-app/src/test.ts",
            "polyfills": "site-app/src/polyfills.ts",
            "tsConfig": "site-app/tsconfig.spec.json",
            "karmaConfig": "site-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["site-app/src/favicon.ico", "site-app/src/assets"],
            "styles": ["site-app/src/styles.scss"],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/site-app/server",
            "main": "site-app/server.ts",
            "tsConfig": "site-app/tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "site-app/src/environments/environment.ts",
                  "with": "site-app/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "site-app:build:development",
              "serverTarget": "site-app:server:development"
            },
            "production": {
              "browserTarget": "site-app:build:production",
              "serverTarget": "site-app:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "site-app:build:production",
              "serverTarget": "site-app:server:production"
            },
            "development": {
              "browserTarget": "site-app:build:development",
              "serverTarget": "site-app:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  }
}
